<%- include("header") -%>
        <div class="box" id="heading">
            <h1 style="color:rgb(248, 248, 248)"><%= nameofday %> </h1>
        </div>
        <div class="box">
        
           <% for(var i=0;i<newItem.length;i++) { %>
               <div class="item">
                <input type="checkbox">
            <p><%=  newItem[i]  %></p>
        </div>
         <% } %>
          <form class="item" method="post" action="/">
        <input type="text" name="newItem" placeholder="Enter new Item" autocomplete="off"/>
        <button type="submit" name="button">+</button>
        </form>
        </div>
        <%- include("footer") -%>